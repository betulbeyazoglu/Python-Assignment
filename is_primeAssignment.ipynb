   ],
   "source": [
    "is_prime = int(input(\"Please enter a number: \"))\n",
    "count = 0\n",
    "for i in range (1, is_prime):\n",
    "    if is_prime%i==0:\n",
    "        count+=1\n",
    "if count>=2:\n",
    "    print(f\"{is_prime} is not a prime number\")\n",
    "else:\n",
    "    print(f\"{is_prime} is a prime number\")"
   ]
